{
  "name": "eslint-plugin-package-json-dependencies",
  "version": "1.0.18",
  "description": "A plugin for package json dependencies",
  "main": "dist",
  "scripts": {
    "build": "tsc",
    "pretest": "npm run build",
    "test": "jest",
    "posttest": "npm run lint",
    "lint": "eslint . --ext json",
    "format": "npx prettier --write ."
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:idan-at/eslint-plugin-package-json-dependencies.git"
  },
  "keywords": [
    "eslint",
    "package-json",
    "types",
    "typescript",
    "dependencies",
    "json"
  ],
  "author": "Idan Attias",
  "license": "MIT",
  "devDependencies": {
    "@types/dedent": "~0.7.0",
    "@types/eslint": "~8.4.10",
    "@types/esprima": "~4.0.0",
    "@types/estree": "~1.0.3",
    "@types/jest": "~29.2.0",
    "@types/lodash": "~4.14.0",
    "@types/micromatch": "~4.0.0",
    "@types/node": "~18.11.0",
    "@types/semver": "~7.5.2",
    "dedent": "~0.7.0",
    "eslint": "~8.28.0",
    "eslint-plugin-self": "~1.2.0",
    "jest": "~29.3.0",
    "ts-jest": "~29.0.0",
    "ts-node": "~10.9.0",
    "typescript": "~4.9.0"
  },
  "dependencies": {
    "comment-json": "~4.2.0",
    "esprima": "~4.0.0",
    "lodash": "~4.17.0",
    "micromatch": "~4.0.0",
    "semver": "~7.5.2"
  }
}
